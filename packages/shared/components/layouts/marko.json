{
  "taglib-imports": [
    "./content/marko.json",
    "./published-content/marko.json",
    "./website-section/marko.json"
  ],
  "<shared-subscribe-page-layout>": {
    "template": "./subscribe.marko",
    "<newsletter-section>": {},
    "<magazine-section>": {},
    "@description": "string"
  },
  "<shared-default-page-layout>": {
    "template": "./default.marko",
    "<section>": {},
    "@title": {
      "type": "string",
      "required": true
    },
    "@type": {
      "type": "string",
      "required": true
    },
    "@description": "string"
  },
  "<shared-home-page-layout>": {
    "template": "./home.marko",
    "<head>": {},
    "<in-head>": {},
    "<above-container>": {},
    "<page>": {},
    "<below-page>": {},
    "<foot>": {},
    "@id": {
      "type": "string",
      "required": true
    },
    "@alias": {
      "type": "string",
      "required": true
    },
    "@name": {
      "type": "string",
      "required": true
    },
    "@page-node": {
      "type": "object",
      "required": true
    },
    "@ad-slots": "function",
    "@with-oop-ads": "boolean",
    "@with-footer-ad": "boolean"
  }
}
