$ const { id, alias, name, pageNode } = input;

<global-website-section-feed-layout
  id=id
  alias=alias
  name=name
  page-node=pageNode
>
  <@section>
    <theme-callout-cards-block>
      <@slot>
        <global-tps-used-truck-guide-promo-block />
      </@slot>
      <@slot>
        <global-white-papers-block />
      </@slot>
    </theme-callout-cards-block>
  </@section>

  <@section>
    <theme-client-side-most-popular-block />
  </@section>
</global-website-section-feed-layout>
